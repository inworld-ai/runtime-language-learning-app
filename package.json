{
  "name": "language-learning",
  "version": "1.0.0",
  "main": "dist/backend/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "dev": "nodemon --exec \"tsx backend/server.ts\" --ext ts",
    "test-stt": "tsx backend/test-stt.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "inworld",
    "ai",
    "language-learning",
    "spanish"
  ],
  "author": "Inworld AI",
  "license": "MIT",
  "description": "Learn Spanish with AI",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tsconfig/node-lts": "^22.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.12",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "@inworld/runtime": "^0.8.0",
    "anki-apkg-export": "^4.0.3",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.0"
  }
}
